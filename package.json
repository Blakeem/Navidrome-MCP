{
  "name": "navidrome-mcp",
  "version": "1.1.1",
  "description": "MCP server for interacting with Navidrome music server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ci": "SKIP_INTEGRATION_TESTS=true MOCK_ONLY_TESTS=true NAVIDROME_URL=http://ci-dummy:4533 NAVIDROME_USERNAME=ci-user NAVIDROME_PASSWORD=ci-pass vitest run",
    "test:all": "pnpm test:run && pnpm check:all",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "check:dead-code": "ts-unused-exports tsconfig.analysis.json --excludePathsFromReport=dist/",
    "check:all": "pnpm lint && pnpm typecheck && pnpm check:dead-code",
    "prepare": "pnpm build",
    "inspector": "npx @modelcontextprotocol/inspector node dist/index.js",
    "inspector:dev": "npx @modelcontextprotocol/inspector tsx src/index.ts"
  },
  "keywords": [
    "mcp",
    "navidrome",
    "music",
    "subsonic",
    "api",
    "server"
  ],
  "author": "",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blakeem/Navidrome-MCP.git"
  },
  "bugs": {
    "url": "https://github.com/Blakeem/Navidrome-MCP/issues"
  },
  "homepage": "https://github.com/Blakeem/Navidrome-MCP#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "bin": {
    "navidrome-mcp": "./dist/index.js"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "dotenv": "^16.4.7",
    "file-type": "^19.5.0",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-unused-exports": "^11.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
